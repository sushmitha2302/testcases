 res[curr.earthquake_region_code].activeTIV += (curr.active_locations_aggregate?.total_insured_value || 0); // Changed here
    res[curr.earthquake_region_code].active_location_count += (curr.active_locations_aggregate?.location_count || 0); // Changed here
    const sumofActiveandProspectTIV: number = (curr.active_locations_aggregate?.total_insured_value || 0) + (curr.prospect_locations_aggregate?.total_insured_value || 0); // Changed here
    res[curr.earthquake_region_code].prospectTIV += sumofActiveandProspectTIV;
    const sumofActiveandProspectLocations: number =
      (curr.active_locations_aggregate?.location_count || 0) + (curr.prospect_locations_aggregate?.location_count || 0); // Changed here
    res[curr.earthquake_region_code].prospect_location_count +=
      sumofActiveandProspectLocations;
    return res;
  }, {});
